<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jenkinsapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.jenkinsapp.MainMenu"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.jenkinsapp.Project1"
            android:label="@string/title_activity_project1" >
        </activity>
        <activity
            android:name="com.example.jenkinsapp.Project1buildq"
            android:label="@string/title_activity_project1buildq" >
        </activity>
        <activity
            android:name="com.example.jenkinsapp.BuildHistory"
            android:label="@string/title_activity_build_history" >
        </activity>
        <activity
            android:name="com.example.jenkinsapp.BuildStatus"
            android:label="@string/title_activity_build_status" >
        </activity>
        <activity
            android:name="com.example.jenkinsapp.BuildQueue"
            android:label="@string/title_activity_build_queue" >
        </activity>
        <activity
            android:name="com.example.jenkinsapp.My_project"
            android:label="@string/title_activity_my_project" >
        </activity>
        <activity
            android:name="com.example.jenkinsapp.My_project_menu"
            android:label="@string/title_activity_my_project_menu" >
